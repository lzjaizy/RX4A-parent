<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rx.authPlat.tenant.dao.PostDataMapper" >

    <insert id="postData" parameterType="com.rx.authPlat.tenant.entity.PostDatasEntity" >
        insert into post_data ( name,ini,date,remark,intro
        )
        values ( #{name,jdbcType=VARCHAR}, #{ini,jdbcType=VARCHAR},#{date,jdbcType=VARCHAR},#{remark,jdbcType=VARCHAR},#{intro,jdbcType=VARCHAR}
        )
    </insert>

</mapper>